# Groq API Key - REEMPLAZA CON TU CLAVE REAL
GROQ_API_KEY=
# Modelo primario (cambia a modelo OSS más potente)
GROQ_MODEL=openai/gpt-oss-12b
# Lista de fallbacks (separada por comas). Orden de prueba.
GROQ_FALLBACK_MODELS=llama-3.3-70b-versatile,openai/gpt-oss-20b
# Temperatura por defecto
GROQ_TEMPERATURE=0.2
# Modelo de embeddings (más potente)
EMBED_MODEL=BAAI/bge-m3
EMBED_NORMALIZE=true
METRIC_TYPE=IP
# Configuración de puertos (opcional)
API_PORT=8000
CHAINLIT_PORT=8080
